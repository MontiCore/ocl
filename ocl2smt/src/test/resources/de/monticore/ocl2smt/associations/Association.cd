import java.lang.* ;

classdiagram Association{
  class Auction {
    int auctionIdent;
  }


  class Enterprise {
   String name ;
  }

  class  Person{
   int age ;
   int id;
  }

  association [1] Auction(auction)     --     (person)Person [1..*];
  association [*] Person(person)      --     (parent)Person [1] ;
  association [*] Person            --      Enterprise[0..1] ;


}
