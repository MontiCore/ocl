package invariants;

ocl comparisons {
  inv(int a, int b):
    a > b &&
    a >= b &&
    a < b &&
    a <= b &&
    a == b &&
    a != b;

  inv(Integer a, Integer b):
    a > b &&
    a >= b &&
    a < b &&
    a <= b &&
    a == b &&
    a != b;
}
